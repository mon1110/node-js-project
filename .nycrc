{
  "cache": false,
  "check-coverage": true,
  "extension": [
    ".js",
    ".ts"
  ],
  "include": [
    "src/**/*.js",
    "src/**/*.ts"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.js",
    "**/*.test.ts",
    "src/tests/**"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true
}
